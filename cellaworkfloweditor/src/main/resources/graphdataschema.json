{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Represents the schema of the JSON communicated to the display.",
  "definitions": {
    "position": {
      "type": "object",
      "properties": {
        "x": {
          "type": "integer"
        },
        "y": {
          "type": "integer"
        }
      }
    },
    "condition": {
      "type": "object",
      "properties": {
        "contextfield": {"type": "string"},
        "fieldvalue": {"type": "string"}
      }
    },
    "property-value": {
      "type": "object",
      "properties": {
        "key": {"type": "string"},
        "value": {"type": "string"}
      }
    },
    "nodetype": {
      "type": "object",
      "properties": {
        "data": {
          "type": "object",
          "properties": {
            "id": {"type": "string"},
            "processor": {"type": "string"},
            "name": {"type": "string"},
            "type": {"type": "string"},
            "conditional": {"type": "#/definitions/conditional"},
            "configuration": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/property-value"
              }
            }
          }
        },
        "position": {"$ref": "#/definitions/position"},
        "removed": {
          "type": "boolean"
        },
        "selected": {
          "type": "boolean"
        },
        "selectable": {
          "type": "boolean"
        },
        "locked": {
          "type": "boolean"
        },
        "grabbable": {
          "type": "boolean"
        },
        "classes": {
          "type": "string"
        }
      }
    },
    "edgetype": {
      "type": "object",
      "properties": {
        "data": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "weight": {
              "type": "integer"
            },
            "source": {
              "type": "string"
            },
            "target": {
              "type": "string"
            }
          }
        },
        "removed": {
          "type": "boolean"
        },
        "selected": {
          "type": "boolean"
        },
        "selectable": {
          "type": "boolean"
        },
        "locked": {
          "type": "boolean"
        },
        "grabbable": {
          "type": "boolean"
        },
        "classes": {
          "type": "string"
        }
      }
    }
  },
  "type": "object",
  "properties": {
    "format_version": {"type": "string"},
    "generated_by": {"type": "string"},
    "target_cytoscapejs_version": {"type": "string"},
    "data": {
      "type": "object",
      "properties": {
        "selected": {"type": "boolean"},
        "Annotations": {"type": "array", "items":{"type": "string"}},
        "shared_name": {"type": "string"},
        "SUID": {"type": "integer"},
        "name": {"type": "string"}
      }
    },
    "elements": {
      "type": "object",
      "properties": {
        "nodes": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/nodetype"
          }
        },
        "edges": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/edgetype"
          }
        }
      }
    }
  }
}
